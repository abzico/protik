{
  "manifest_version": 2,

  "name": "protik",
  "short_name": "protik",
  "description": "This extension protects your flow of productivity from other people's views in Twitter feeds.",
  "author": "Wasin Thonkaew",
  "version": "0.1.1",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png",
    "512": "icon512.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click to configure protik"
  },
  "content_scripts" : [
    {
      "js": ["inject.js"],
      "matches": ["https://twitter.com/*"]
    }
 ],
 "key": "",
 "oauth2": {
  "client_id": "189524929570-cpr5nr7hnd2daqqhdcvqf6m8qlgmerl7.apps.googleusercontent.com",
  "scopes": [
    "https://www.googleapis.com/auth/chromewebstore.readonly"
  ]
 },
  "permissions": [
    "identity",
    "storage",
    "tabs",
    "https://www.googleapis.com/"
  ]
}